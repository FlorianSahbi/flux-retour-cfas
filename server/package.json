{
  "name": "mna-flux-retour-cfas",
  "version": "1.0.8",
  "description": "[MNA] Flux retour des cfas",
  "repository": "https://github.com/mission-apprentissage/flux-retour-cfas.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "mocha --recursive --exit --timeout 5000 tests/",
    "lint": "eslint src/ tests/",
    "docker:start": "docker-compose up -d --build --force-recreate",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose kill && docker system prune --force --volumes",
    "seed:users": "node src/jobs/seed/seedUsers.js",
    "seed:sample": "node src/jobs/seed/seedSample.js",
    "seed:randomizedSample": "node src/jobs/seed/seedRandomizedSample.js",
    "clear:all": "node src/jobs/clear/clearAll.js",
    "clear:statutsCandidats": "node src/jobs/clear/clearStatutsCandidats.js",
    "clear:logs": "node src/jobs/clear/clearLogs.js",
    "clear:users": "node src/jobs/clear/clearUsers.js",
    "stats": "node src/jobs/stats/index.js",
    "ds:import": "node src/jobs/enqueteDs/import.js",
    "ds:stats": "node src/jobs/enqueteDs/buildStats.js",
    "ds:updateSib": "node src/jobs/enqueteDs/updateSib.js",
    "ds:buildYmagClients": "node src/jobs/enqueteDs/buildYmagClientList.js",
    "ds:buildGestiClients": "node src/jobs/enqueteDs/buildGestiClientList.js",
    "run:tests": "node src/jobs/test/index.js"
  },
  "dependencies": {
    "async": "3.2.0",
    "axios": "0.20.0",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "cli-progress": "3.8.2",
    "compose-middleware": "5.0.1",
    "config": "3.3.1",
    "csv-parse": "4.14.1",
    "env-var": "6.1.1",
    "express": "4.17.1",
    "faker": "5.1.0",
    "fs-extra": "9.0.0",
    "iconv-lite": "0.6.2",
    "joi": "17.2.1",
    "json2csv": "5.0.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "moment": "2.26.0",
    "mongoose": "5.10.2",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "randexp": "0.5.3",
    "requestretry": "4.1.1",
    "sha512crypt-node": "0.1.0",
    "tail": "2.0.4",
    "xlsx": "0.16.1"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "axiosist": "0.9.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "mocha": "7.1.2",
    "nodemon": "2.0.3",
    "prettier": "2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/preventSensibleFilesCommit.sh && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
